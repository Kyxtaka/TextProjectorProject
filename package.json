{
  "name": "online-text-projector",
  "version": "1.0.0",
  "description": "A web solution to broadcast text, song, bible verses in realtime within the internet",
  "homepage": "https://github.com/Kyxtaka/TextProjectorProject#readme",
  "private": true,
  "bugs": {
    "url": "https://github.com/Kyxtaka/TextProjectorProject/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kyxtaka/TextProjectorProject.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "Hikarizsu (Nathan RANDRIANTSOA)",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "apps:api:start": "cd apps/online-text-projector-api && npm run start",
    "apps:api:start:dev": "cd apps/online-text-projector-api && npm run start:dev",
    "apps:api:start:test-server": "cd apps/online-text-projector-api && npm run start:test-server",
    "apps:api:start:debug": "cd apps/online-text-projector-api && npm run start:debug",
    "apps:api:seed:users:dev": "cd apps/online-text-projector-api && npm run seed:users:dev",
    "apps:api:seed:users:test": "cd apps/online-text-projector-api && npm run seed:users:test",
    "apps:api:seed:users:prod": "cd apps/online-text-projector-api && npm run seed:users:prod",

    "apps:client:start": "cd apps/online-text-projector-client && npm run start",
    "apps:client:start:dev": "cd apps/online-text-projector-client && npm run dev"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "inquirer": "^13.2.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/inquirer": "^9.0.9"
  }
}
